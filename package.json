{
  "author": "John Ferlito <johnf@inodes.org> (http://inodes.org)",
  "bugs": {
    "url": "https://github.com/echohubio/hubber/issues"
  },
  "build": {
    "appId": "io.echohub.hubber",
    "copyright": "Copyright Â© 2017 EchoHub",
    "linux": {
      "target": [
        "dir",
        "deb",
        "rpm"
      ]
    },
    "mac": {
      "category": "public.app-category.productivity"
    },
    "nsis": {
      "license": "LICENSE"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://packages.echohub.io/${os}"
      },
      {
        "provider": "s3",
        "bucket": "packages.echohub.io",
        "path": "${os}"
      },
      {
        "provider": "github"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "dependencies": {
    "architect": "^0.1.13",
    "babel-cli": "^6.4.0",
    "bootstrap": "^3.3.7",
    "electron-log": "^2.2.6",
    "electron-oauth2": "johnf/electron-oauth2",
    "electron-updater": "^1.16.0",
    "hubber-iot": "^1.0.1",
    "hubber-plugins": "^1.0.2",
    "phetch": "0.0.10",
    "raven-js": "^3.15.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-persist": "^4.8.0",
    "redux-persist-node-storage": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.4.15"
  },
  "description": "EchoHub Hubber",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react-app": "^3.0.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "electron": "^1.6.10",
    "electron-builder": "^18.1.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-publisher-s3": "^18.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "minimist": "^1.2.0",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^4.1.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "homepage": "https://www.echohub.io",
  "license": "ISC",
  "main": "app/main.js",
  "name": "hubber",
  "private": true,
  "repository": {
    "type": "git",
    "url": "echohubio/hubber"
  },
  "scripts": {
    "build": "yarn run build-main && yarn run build-renderer",
    "build-main": "cross-env NODE_ENV=production webpack --config config/webpack.config.electron.babel.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config config/webpack.config.production.babel.js --progress --profile --colors",
    "dev": "yarn run hot-server -- --start-hot",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "lint": "eslint .",
    "pack": "yarn run build && build --dir",
    "dist": "yarn run build && build",
    "release": "package"
  },
  "version": "0.0.4"
}
